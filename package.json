{
  "name": "inferno-milligram-boilerplate",
  "version": "1.0.0",
  "description": "a lightweight minimal boilerplate using Inferno JS and Milligram CSS",
  "main": "dist/index.js",
  "web": "dist/index.js",
  "module": "src/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "clean:dev": "rimraf ./dist-dev",
    "build": "npm run clean && webpack --progress --profile --bail",
    "build:dev": "npm run clean:dev && webpack --progress --profile",
    "start": "webpack-dev-server --host 0.0.0.0 --port=3000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "inferno": "^3.0.5",
    "inferno-component": "^3.0.5",
    "inferno-router": "^3.0.5",
    "milligram": "^1.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-inferno": "^3.2.0",
    "babel-preset-env": "^1.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "optimize-js-plugin": "0.0.4",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4"
  },
  "author": "khaled <khaledelansari@gmail.com>",
  "license": "ISC"
}
